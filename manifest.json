{
    "name": "BSMRMU CRX",
    "version": "0.0.0.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "manifest/background/script.js"
    },
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "https://bsmrmu.edu.bd/",
      "https://bsmrmu.edu.bd/notices/General%20Notice",
      "https://bsmrmu.edu.bd/notices/Academic%20Notice"
    ],
    "icons": {
      "16": "manifest/icons/16.png",
      "32": "manifest/icons/32.png",
      "192": "manifest/icons/192.png",
      "512": "manifest/icons/512.png"
    },
    "action": {
      "default_title": "BSMRMU CRX",
      "default_popup": "popup/index.html",
      "default_icon": {
        "16": "manifest/icons/16.png",
        "32": "manifest/icons/32.png",
        "192": "manifest/icons/192.png",
        "512": "manifest/icons/512.png"
      }
    },
    "chrome_url_overrides": {
      "newtab": "newtab/index.html"
    }
  }